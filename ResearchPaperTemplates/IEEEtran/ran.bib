  Keywords                 = {3-(2-Pyridyl)coumarin derivatives,Crystal structure,Fluorescence,Synthesis},
  Owner                    = {a},
  Timestamp                = {2016.01.07},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S1010603010002200}
}

